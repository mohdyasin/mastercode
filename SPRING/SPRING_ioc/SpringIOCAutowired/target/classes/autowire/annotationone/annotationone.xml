<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

<bean class="org.springframework.beans.factory.annotation.AutowiredAnnotationBeanPostProcessor"/>
	
	<bean id="CustomerBean" class="autowire.beans.annotation.one.Customer">
		<property name="action" value="buy"></property>
		<property name="type" value="1"></property>

	</bean>

	<bean id="PersonBean" class="autowire.beans.annotation.one.Person">
		<property name="name" value="neo"></property>
		<property name="address" value="Address ABC"></property>
		<property name="age" value="29"></property>
	</bean>

</beans>